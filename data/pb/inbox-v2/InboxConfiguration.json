{
  "tenantId": "pb",
  "moduleName": "INBOX",
  "InboxQueryConfiguration": [
    {
      "module": "estimate-service",
      "index": "estimate-inbox",
      "allowedSearchCriteria": [
        {
          "name": "tenantId",
          "path": "Data.tenantId.keyword",
          "isMandatory": true,
          "operator": "EQUAL"
        },
        {
          "name": "status",
          "path": "Data.currentProcessInstance.state.uuid.keyword",
          "isMandatory": false
        },
        {
          "name": "estimateId",
          "path": "Data.estimateNumber.keyword",
          "isMandatory": false
        },
        {
          "name": "department",
          "path": "Data.executingDepartment.keyword",
          "isMandatory": false
        },
        {
          "name": "typeOfWork",
          "path": "Data.typeOfWork.keyword",
          "isMandatory": false
        },
        {
          "name": "projectId",
          "path": "Data.project.projectNumber",
          "isMandatory": false
        },
        {
          "name": "fromProposalDate",
          "path": "Data.proposalDate.keyword",
          "isMandatory": false,
          "operator": "GTE"
        },
        {
          "name": "toProposalDate",
          "path": "Data.proposalDate.keyword",
          "isMandatory": false,
          "operator": "LTE"
        },
        {
          "name": "createdBy",
          "path": "Data.auditDetails.createdBy",
          "isMandatory": false
        }
      ],
      "sortBy": {
          "path": "Data.auditDetails.createdTime",
          "defaultOrder": "ASC"
      },
      "sourceFilterPathList": ["Data.currentProcessInstance", "Data.auditDetails", "Data.additionalDetails", "Data.tenantId","Data.project","Data.estimateDetails"]
    },
    {
      "module": "muster-roll-service",
      "index": "muster-inbox",
      "allowedSearchCriteria": [
        {
          "name": "tenantId",
          "path": "Data.tenantId.keyword",
          "isMandatory": true,
          "operator": "EQUAL"
        },
        {
          "name": "status",
          "path": "Data.currentProcessInstance.state.uuid.keyword",
          "isMandatory": false
        },
        {
          "name": "musterRollStatus",
          "path": "Data.musterRollStatus.keyword",
          "isMandatory": false
        },
        {
          "name": "startDate",
          "path": "Data.startDate.keyword",
          "isMandatory": false,
          "operator": "GTE"
        },
        {
          "name": "endDate",
          "path": "Data.endDate.keyword",
          "isMandatory": false,
          "operator": "LTE"
        },
        {
          "name": "attendanceRegisterName",
          "path": "Data.additionalDetails.attendanceRegisterName.keyword",
          "isMandatory": false
        }
      ],
      "sortBy": {
        "path": "Data.auditDetails.createdTime",
        "defaultOrder": "ASC"
      },
      "sourceFilterPathList": ["Data.tenantId" ,"Data.id", "Data.musterRollNumber", "Data.musterRollStatus", "Data.startDate", "Data.endDate", "Data.additionalDetails", "Data.currentProcessInstance", "Data.auditDetails"]
    }
  ]
}
